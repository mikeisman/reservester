<!DOCTYPE html>
<html>
<head>
  <title>Reservester</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

    <div class="nav">
        <% if owner_signed_in? %>
            <%= link_to "Log out #{current_owner.email}", destroy_owner_session_path %>
        <% else %>
            <%= link_to "Log in", new_owner_session_path %>
        <% end %>
    </div>

    <p id="notice" class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>


    <%= yield %>

</body>
</html>
